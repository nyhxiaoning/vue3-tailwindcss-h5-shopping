{
	"name": "vue3-shopping-airi",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"serve": "vite preview",
		"prepare": "husky install",
		"pre-commit": "yarn lint-staged",
		"lint": "eslint src --ext .ts,.vue",
		"lint:fix": "eslint --fix --ext src --ext .ts,.vue",
		"format": "prettier --write \"src/**/*.vue\" \"src/**/*.ts\"",
		"commit": "git-cz"
	},
	"dependencies": {
		"genji-es": "^1.0.2",
		"vant": "^3.0.17",
		"vue": "^3.0.5",
		"vue-router": "4"
	},
	"devDependencies": {
		"@better-scroll/core": "^2.3.1",
		"@commitlint/cli": "^12.1.4",
		"@types/axios": "^0.14.0",
		"@types/node": "^15.6.0",
		"@vitejs/plugin-vue": "^1.2.2",
		"@vue/compiler-sfc": "^3.0.5",
		"@vueuse/core": "^4.11.0",
		"autoprefixer": "^10.2.5",
		"axios": "^0.21.1",
		"commitizen": "^4.2.3",
		"commitlint-config-cz": "^0.13.2",
		"cz-customizable": "^6.3.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"postcss": "^8.3.0",
		"prettier": "^2.3.1",
		"tailwindcss": "^2.1.2",
		"typescript": "^4.1.3",
		"vite": "^2.3.3",
		"vite-plugin-pwa": "^0.8.1",
		"vite-plugin-style-import": "^0.10.0",
		"vue-tsc": "^0.0.24",
		"xieyezi-storage": "^1.0.4"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	}
}
